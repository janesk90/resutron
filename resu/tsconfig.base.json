{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@resu/accolade-types/domain": [
        "libs/accolade-types/domain/src/index.ts"
      ],
      "@resu/accolades-to-persons/domain": [
        "libs/accolades-to-persons/domain/src/index.ts"
      ],
      "@resu/accolades/domain": ["libs/accolades/domain/src/index.ts"],
      "@resu/companies/domain": ["libs/companies/domain/src/index.ts"],
      "@resu/domain": ["libs/domain/src/index.ts"],
      "@resu/entity-validators": ["libs/entity-validators/src/index.ts"],
      "@resu/persons/domain": ["libs/persons/domain/src/index.ts"],
      "@resu/position-notes/domain": [
        "libs/position-notes/domain/src/index.ts"
      ],
      "@resu/positions/domain": ["libs/positions/domain/src/index.ts"],
      "@resu/resume-entity": ["libs/resume-entity/src/index.ts"],
      "@resu/shared/domain": ["libs/shared/domain/src/index.ts"],
      "@resu/skills-to-persons/domain": [
        "libs/skills-to-persons/domain/src/index.ts"
      ],
      "@resu/skills/domain": ["libs/skills/domain/src/index.ts"]
    },
    "types": ["node"],
    "typeRoots": ["../node_modules/@types"]
  },
  "exclude": ["node_modules", "tmp"]
}
